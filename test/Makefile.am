## Process this file with automake to produce Makefile.in
##
## Notes:
##
##   Test files are named similar to "system_under.N.test.t". The ".t" suffix identifies
##   files that will have a test suite generated by testgen.sh. The ".test" suffix is
##   used to identify the files that will be deleted by "make clean". "N" indicates the
##   order that the tests are intended to be run.

SUFFIXES = .t
TEST_LOG_DRIVER = env AM_TAP_AWK='$(AWK)' $(SHELL) $(top_srcdir)/build-aux/tap-driver.sh
TESTS = $(check_PROGRAMS)
EXTRA_DIST = $(TESTS)
CLEANFILES = *.test.c

.t.c:
	./testgen.sh $< $@

check_PROGRAMS =          \
	adt_object.0.test     \
	adt_object.1.test     \
	adt_collection.0.test \
	adt_sequence.0.test   \
	adt_list.0.test       \
	adt_list.1.test       \
	adt_list.2.test       \
	adt_list.3.test       \
	adt_array.0.test      \
	adt_array.1.test      \
	adt_array.2.test      \
	adt_array.3.test      \
	adt_pair.0.test       \
	adt_pair.1.test       \
	adt_map.0.test        \
	adt_map.1.test        \
	adt_map.2.test

adt_object_0_test_SOURCES = sut_test.c adt_object.0.test.c $(top_srcdir)/src/adt_object.h
adt_object_0_test_CFLAGS = -I$(top_srcdir)/src
adt_object_0_test_LDADD = $(top_srcdir)/src/libadt.a
adt_object_0_test_LIBS = -ladt

